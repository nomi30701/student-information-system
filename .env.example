# =======================
# environment variables Example file
# rename to .env and fill in the actual values
# =======================

# SA account for MS SQL Server
SA_PASSWORD=your_secure_database_password_here
MSSQL_PID=Express

# Backend JWT Settings
JWT_SECRET_KEY=your_jwt_secret_key_at_least_32_characters_long
JWT_ISSUER=YourApplicationName
JWT_AUDIENCE=YourUserGroup
JWT_EXPIRY_MINUTES=60

# Database connection string components
DB_SERVER=database
DATABASE_NAME=Std_info_sys
DB_PORT=1433
DB_USER_ID=stdsysadmin
DB_USER_PASSWORD=YourUserPassword

# CORS 
ALLOWED_ORIGINS=http://localhost,http://localhost:80,https://yourdomain.com